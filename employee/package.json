{
  "name": "core-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "commit": "yarn lint && git-cz",
    "prebuild": "rimraf dist",
    "nest": "node_modules/.bin/nest",
    "build": "yarn run nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "yarn run nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "start:kong": "docker-compose -f docker-compose-kong.yaml up -d",
    "start:from:backup": "docker-compose -f docker-compose-from-backup.yaml up -d",
    "start:db": "docker-compose -f docker-compose-db.yaml up -d",
    "down:kong": "docker-compose -f docker-compose-kong.yaml down --remove-orphans",
    "down:db": "docker-compose -f docker-compose-db.yaml down --remove-orphans",
    "down": "yarn down:kong && yarn down:db",
    "migration:create": "yarn typeorm migration:create",
    "migration:run:employee": "yarn typeorm migration:run -d src/typeormconfig.ts",
    "migration:run:authentication": "yarn typeorm migration:run -d src/typeormconfig.ts",
    "start:local:authentication": "export NODE_ENV=local && yarn migration:run:authentication && nest start --watch",
    "start:local:employee": "export NODE_ENV=local && yarn migration:run:employee && yarn nest start --watch",
    "start:local:scheduler": "export NODE_ENV=local && yarn nest start --watch scheduler",
    "migration:run:authentication:deploy:mode": "./node_modules/typeorm/cli.js migration:run -d dist/apps/authentication/src/typeormconfig.js",
    "migration:run:employee:deploy:mode": "./node_modules/typeorm/cli.js migration:run -d dist/apps/employee/src/typeormconfig.js",
    "start:dev:authentication": "export NODE_ENV=development && yarn migration:run:authentication:deploy:mode && node dist/apps/authentication/src/main.js",
    "start:dev:employee": "export NODE_ENV=development && yarn migration:run:employee:deploy:mode && node dist/apps/employee/src/main.js",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "start:dev:scheduler": "export NODE_ENV=development && node dist/apps/scheduler/src/main.js"
  },
